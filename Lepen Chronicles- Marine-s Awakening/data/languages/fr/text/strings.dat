text{ key = "intro.0", value = "Il est une histoire mythique qu'on raconte,$parmi tant d'autres légendes..." }
text{ key = "intro.1", value = "Il y a bien longtemps, existait un royaume$ou l'on disait qui cachait le pouvoir des Dieux" }
text{ key = "intro.2", value = "Ce royaume habitait le " }
text{ key = "intro.3", value = "Une guerre eclata et les forces du mal voulaient s'emparer$de la triforce. Les forces royales diminuaient de jour en jour. " }
text{ key = "intro.4", value = "La Princesse du Royaume decida en dernier recours de confier$une tache a un garcon tout de vert vetu." }
text{ key = "intro.5", value = "Test" }
text{ key = "intro.6", value = "Profitant de l'absence du heros, un homme noir d'esprit,$Ganondorf vena capturer la Princesse et derober la Triforce." }
text{ key = "intro.7", value = "Ayant recu par les deeses un pouvoir destructeur, il$mena le royaume vers le chaos et la desolation." }
text{ key = "intro.8", value = "Ayant recu par les deeses un pouvoir destructeur, il$mena le royame vers le chaos et la desolation." }
